LANGUAGE "SIC 1.0".
SCROLL send_back_demo
MODE CHANT.

WORK ADD WITH SIGIL UNUSED AS TEXT:
  LET SIGIL left BE "left ".
  LET SIGIL right BE "right".
  SEND BACK left + right.
ENDWORK.

WORK MAIN WITH SIGIL UNUSED AS TEXT:
  SAY: "Testing SEND BACK / + demo.".

  LET SIGIL result BE SUMMON WORK ADD WITH SIGIL UNUSED.

  SAY: "Result: " + result.
ENDWORK.
